package code13_quizans;

public class Main {

	public static void main(String[] args) {
		Wizard w = new Wizard();
		Wand wand = new Wand();
		Hero h = new Hero();
		w.setHp(30);
		w.setMp(50);
		w.setName("wizard");
		w.setWand(wand);

		h.setHp(40);
		h.setName("minato");

		wand.setName("ニワトコの杖");
		wand.setPower(30);

		w.heal(h);
	}
}
